{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${modVersion}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"OffsetMonkey538"
	],
	"contact": {
		"sources": "https://github.offsetmonkey538.top/${modId}"
	},
	"license": "${modLicense}",
	"icon": "assets/${modId}/icon.png",
	"environment": "*",
